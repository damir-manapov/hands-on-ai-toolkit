{
  "name": "hands-on-ai-toolkit",
  "version": "0.1.0",
  "description": "Hands-on project for learning AI Toolkit by Ostris - LoRA training for diffusion models (FLUX, SDXL, Wan, etc.)",
  "author": "Damir Manapov",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "compose:pull": "docker compose -f compose/docker-compose.yml pull",
    "compose:up": "docker compose -f compose/docker-compose.yml up --wait",
    "compose:down": "docker compose -f compose/docker-compose.yml down",
    "compose:restart": "docker compose -f compose/docker-compose.yml restart",
    "compose:reset": "docker compose -f compose/docker-compose.yml down -v --remove-orphans",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "biome:migrate": "biome migrate --write"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "@types/node": "^25.2.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {},
  "packageManager": "pnpm@10.29.3",
  "engines": {
    "node": ">=22"
  }
}
